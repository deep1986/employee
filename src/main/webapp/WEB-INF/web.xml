<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>registration</servlet-name>
  	<display-name>Employee_details</display-name>
  	<description></description>
  	<servlet-class>com.seh.Employee_details</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>empUpdate</servlet-name>
  	<display-name>EmpUpdate</display-name>
  	<description></description>
  	<servlet-class>com.seh.EmpUpdate</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>empDelete</servlet-name>
  	<display-name>EmpDelete</display-name>
  	<description></description>
  	<servlet-class>com.seh.EmpDelete</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>registration</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>empUpdate</servlet-name>
  	<url-pattern>/empUpdate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>empDelete</servlet-name>
  	<url-pattern>/empDelete</url-pattern>
  </servlet-mapping>
</web-app>
